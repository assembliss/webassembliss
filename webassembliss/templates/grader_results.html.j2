{% extends "base.html.j2" %}
{% block title %} - Grader Results{% endblock %}
{% block content %}
  <div class="container">
    <div class="p-5 bg-body-tertiary rounded-3">
      <h1 class="text-center text-body-emphasis">Grader Results</h1>
      <div class="text-center">
        <button type="submit" class="btn btn-danger" onclick='download_file("test.json", "[]", "application/json")'>Download file for submission</button>
      </div>
            
      <h2>Total Score: {{ "%.2f"| format(results.submission.total * 100) }}</h2>
      

      <h2>Project Info</h2>
      <h5>Name: {{ results.submission.project_name }}</h3>
      <h5>Must pass all test cases for a non-zero grade: {{ results.submission.must_pass_all_tests }}</h5>

      
      <h2>Accuracy ({{ "%.2f"| format(results.submission.weights.accuracy * 100)}}%) </h2>
      Category total: {{ "%.2f"| format(results.submission.scores.accuracy * 100) }}
      
      
      <h2>Documentation ({{ "%.2f"| format(results.submission.weights.documentation * 100) }}%)</h2>
      Category total: {{ "%.2f"| format(results.submission.scores.documentation * 100) }}
      
      
      <h2>Source Efficiency ({{ "%.2f"| format(results.submission.weights.source_efficiency * 100) }}%)</h2>
      Instructions written: {{ results.submission.line_count }}
      <br/>
      Category total: {{ "%.2f"| format(results.submission.scores.source_efficiency * 100) }}
      
      
      <h2>Execution Efficiency ({{ "%.2f"| format(results.submission.weights.exec_efficiency * 100) }}%)</h2>
      Instructions executed: {{ results.submission.agg_exec_count }}
      <br/>
      Category total: {{ "%.2f"| format(results.submission.scores.exec_efficiency * 100) }}
      
      
      <div class="accordion" id="accordionPanelsStayOpenExample">
